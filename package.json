{
  "publisher": "BeanLabBYU",
  "name": "vscode-byolad",
  "displayName": "byoLAD",
  "description": "",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.81.0"
  },
  "license": "SEE LICENSE IN LICENSE",
  "repository": {
    "type": "git",
    "url": "https://github.com/beanlab/byoLAD.git"
  },
  "bugs": {
    "url": "https://github.com/beanlab/byoLAD/issues"
  },
  "categories": [
    "Programming Languages",
    "Snippets",
    "Machine Learning",
    "Education"
  ],
  "keywords": [
    "ai",
    "assistant",
    "code review"
  ],
  "activationEvents": [],
  "preview": true,
  "icon": "byo_LAD.png",
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "byoLAD",
      "properties": {
        "vscode-byolad.provider": {
          "type": "string",
          "enum": [
            "OpenAI",
            "Google"
          ],
          "default": "OpenAI",
          "markdownDescription": "You are billed by the provider according to their API usage policies.",
          "order": 0
        },
        "vscode-byolad.model": {
          "type": "string",
          "default": "",
          "markdownDescription": "For OpenAI, format your input like 'gpt-4-32k' or 'gpt-3.5-turbo'. See all available models [here](https://platform.openai.com/docs/models/).\n\nFor Google, format your input like 'text-bison-001' or 'chat-bison-001'. See all available models [here](https://developers.generativeai.google/models/language).",
          "order": 1
        },
        "vscode-byolad.APIKey": {
          "type": "string",
          "default": "",
          "markdownDescription": "Get an API key for OpenAI [here](https://platform.openai.com/account/api-keys).\n\nGet an API key for Google [here](https://developers.generativeai.google/tutorials/setup).",
          "order": 2
        },
        "vscode-byolad.reviewFileCodePrompt": {
          "type": "string",
          "default": "You are acting as GitHub Copilot. Return only code. Review the code and fix any bugs, then return the code.",
          "description": "The prompt to use when reviewing a file",
          "editPresentation": "multilineText",
          "order": 3
        },
        "vscode-byolad.reviewSelectedCodePrompt": {
          "type": "string",
          "default": "You are acting as GitHub Copilot. Return only code. Review the code and fix any bugs, then return the code.",
          "description": "The prompt to use when reviewing selected code",
          "editPresentation": "multilineText",
          "order": 4
        },
        "vscode-byolad.applySuggestions.mode": {
          "type": "string",
          "default": "Manual",
          "enum": [
            "Manual",
            "Auto"
          ],
          "enumDescriptions": [
            "See AI-proposed changes and apply selected changes manually",
            "Apply AI-proposed changes with the option to revert selected changes"
          ],
          "order": 5
        },
        "vscode-byolad.applySuggestions.diffViewPosition": {
          "type": "string",
          "default": "Below",
          "description": "Controls where the diff view should be opened when comparing AI suggestions.",
          "enum": [
            "Current",
            "Beside",
            "Below",
            "Use Merge Conflict Setting"
          ],
          "enumDescriptions": [
            "Open the diff view in the current editor group",
            "Open the diff view next to the current editor group",
            "Open the diff view below the current editor group",
            "Use the value of the merge-conflict.diffViewPosition setting to determine where the diff view for AI suggestions should be opened"
          ],
          "order": 6
        }
      }
    },
    "commands": [
      {
        "command": "hello-world.showHelloWorld",
        "title": "Hello World (React + Vite): Show"
      },
      {
        "command": "vscode-byolad.startConversation",
        "title": "byoLAD — Start Conversation",
        "category": "byoLAD"
      },
      {
        "command": "vscode-byolad.clearConversations",
        "title": "byoLAD — Clear Conversations",
        "category": "byoLAD"
      },
      {
        "command": "vscode-byolad.reviewCode",
        "title": "byoLAD — Review Code",
        "category": "byoLAD",
        "icon": {
          "dark": "./media/icons8-stars-16-white.png",
          "light": "./media/icons8-stars-16-black.png"
        }
      },
      {
        "command": "vscode-byolad.explainCode",
        "title": "byoLAD — Explain Code",
        "category": "byoLAD"
      },
      {
        "command": "vscode-byolad.sendMessage",
        "title": "byoLAD — Send Message",
        "category": "byoLAD"
      },
      {
        "command": "vscode-byolad.diffLatestCodeBlock",
        "title": "byoLAD — Diff Latest Code Block",
        "category": "byoLAD"
      },
      {
        "command": "vscode-byolad.openSettingsCommand",
        "title": "byoLAD — Open Settings",
        "category": "byoLAD"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "vscode-byolad.reviewCode",
          "when": "!editorReadonly && !isInDiffEditor"
        },
        {
          "command": "vscode-byolad.explainCode",
          "when": "!editorReadonly && !isInDiffEditor"
        },
        {
          "command": "vscode-byolad.startConversation",
          "when": "!editorReadonly && !isInDiffEditor"
        },
        {
          "command": "vscode-byolad.clearConversations"
        },
        {
          "command": "vscode-byolad.diffLatestCodeBlock",
          "when": "!editorReadonly && !isInDiffEditor"
        }
      ],
      "editor/context": [
        {
          "when": "editorFocus && !editorReadonly && !isInDiffEditor",
          "command": "vscode-byolad.startConversation",
          "group": "1_modification"
        },
        {
          "when": "editorFocus && !editorReadonly && !isInDiffEditor",
          "command": "vscode-byolad.reviewCode",
          "group": "1_modification"
        },
        {
          "when": "editorFocus && !editorReadonly && !isInDiffEditor",
          "command": "vscode-byolad.explainCode",
          "group": "1_modification"
        }
      ],
      "editor/title": [
        {
          "when": "!editorReadonly && !isInDiffEditor",
          "command": "vscode-byolad.reviewCode",
          "group": "navigation@1"
        }
      ]
    },
    "walkthroughs": [
      {
        "id": "setUp",
        "title": "Set Up for byoLAD",
        "description": "Make sure to set up your API Key and model in the settings.",
        "steps": [
          {
            "id": "apiKey",
            "title": "Set Your API Key and Model",
            "description": "This step will take you to settings, and show you where you can put in your API key and model. (Note: You have to obtain your own api key from a provider of your choice)\n[Set API key](command:vscode-byolad.openSettingsCommand)",
            "media": {
              "markdown": "walkthrough.md"
            },
            "completionEvents": [
              "onCommand:vscode-byolad.openSettingsCommand"
            ]
          }
        ]
      }
    ]
  },
  "scripts": {
    "install:all": "npm install && cd webview-ui && npm install",
    "start:webview": "cd webview-ui && npm run start",
    "build:webview": "cd webview-ui && npm run build",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vscode/test-electron": "^2.3.4",
    "@vscode/vsce": "^2.21.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "prettier": "3.0.3",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@google-ai/generativelanguage": "^1.0.1",
    "google-auth-library": "^9.0.0",
    "openai": "^4.5.0"
  }
}
